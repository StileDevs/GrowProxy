{
  "name": "growproxy",
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start:web": "node ./.output/server/index.mjs",
    "start": "concurrently -p \"[{name}]\" -n \"PROXY,WEB\" -c \"bgGreen.bold,bgBlue.bold\" \"nr proxy:start\" \"nr start:web\"",
    "proxy:start": "tsx proxy/index.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.8",
    "growtopia.js": "^2.0.14",
    "hono": "^4.7.1",
    "ky": "^1.7.5",
    "log4js": "^6.9.1",
    "nuxt": "^3.15.4",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@antfu/ni": "^23.3.1",
    "@types/node": "^22.13.4",
    "concurrently": "^9.1.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
